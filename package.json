{
  "name": "@tailify/eslint-config",
  "version": "3.7.0",
  "description": "Tailify's very own opinionated ESLint configuration",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && npm run test-only",
    "test-only": "jest"
  },
  "repository": "tailify/eslint-config",
  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "license": "UNLICENSED",
  "bugs": "https://github.com/tailify/eslint-config/issues",
  "homepage": "https://github.com/tailify/eslint-config#readme",
  "dependencies": {
    "babel-eslint": "10.0.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-plugin-eslint-comments": "3.1.1",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jest": "22.5.1",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-promise": "4.1.1"
  },
  "devDependencies": {
    "@tailify/eslint-config-legacy": "^1.0.0",
    "@tailify/prettier-config": "^1.0.0",
    "eslint": "^5.9.0",
    "jest": "^24.4.0",
    "prettier": "^1.15.0"
  },
  "peerDependencies": {
    "eslint": "^5.9.0",
    "prettier": "^1.15.0"
  },
  "engines": {
    "node": ">=8.3"
  },
  "greenkeeper": {
    "commitMessages": {
      "dependencyUpdate": "Update ${dependency} to version ${version}",
      "devDependencyUpdate": "Update ${dependency} to version ${version}",
      "dependencyPin": "Pin ${dependency} to ${oldVersion}",
      "devDependencyPin": "Pin ${dependency} to ${oldVersion}"
    },
    "prTitles": {
      "basicPR": "Update ${dependency} to the latest version",
      "groupPR": "Update ${dependency} in group ${group} to the latest version"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
